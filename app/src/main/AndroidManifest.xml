<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jpuyo.android.recyclerviewplayground" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".common.activities.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".recyclerview.nolibraries.NoLibrariesActivity"
            android:label="@string/app_name"/>
        <activity
            android:name=".recyclerview.epoxy.EpoxyActivity"
            android:label="@string/app_name"/>
        <activity
            android:name=".recyclerview.renderers.RenderersActivity"
            android:label="@string/app_name"/>
        <activity
            android:name=".recyclerview.groupie.GroupieActivity"
            android:label="@string/app_name"/>
    </application>

</manifest>